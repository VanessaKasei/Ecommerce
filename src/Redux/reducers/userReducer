const initialState = {
    id: null, // or undefined
    role: null,
  };
  
  const userReducer = (state = initialState, action) => {
    switch (action.type) {
      case 'SET_USER_ID':
        return {
          ...state,
          id: action.payload.id, // Ensure you are saving the ID correctly
          role: action.payload.role,
        };
      default:
        return state;
    }
  };
  